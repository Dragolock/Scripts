local Players = game:GetService("Players")

task.wait(2) -- allow data to load

local function getValue(folder, statName)
    if folder then
        local stat = folder:FindFirstChild(statName)
        if stat and stat:IsA("ValueBase") then
            return tostring(stat.Value)
        end
    end
    return "N/A"
end

local otherDataStats = {
    "Cubes","Generations","GrandTime","LocksLeft","Rank","RerollsLeft",
    "Clicks","ServerClicks","Stars","Title"
}

local secondLeaderStats = {"Earons","Quantum"}
local mainLeaderstats = {"Eons","Time","Universes"}
local lockedStats = {"Eons","Generations","GrandTime","Stars","Time","Universes"}

local result = ""

for _, player in ipairs(Players:GetPlayers()) do
    
    result = result .. "===== " .. player.Name .. " =====\n"

    local otherData = player:FindFirstChild("OtherData")
    local secondStats = player:FindFirstChild("SecondStats")
    local secondLeader = secondStats and secondStats:FindFirstChild("leaderstats")
    local mainLeader = player:FindFirstChild("leaderstats")
    local classesFolder = otherData and otherData:FindFirstChild("Classes")

    -- OtherData stats
    for _, stat in ipairs(otherDataStats) do
        result = result .. stat .. ": " .. getValue(otherData, stat) .. "\n"
    end

    -- SecondStats → leaderstats (Earons / Quantum)
    for _, stat in ipairs(secondLeaderStats) do
        result = result .. stat .. ": " .. getValue(secondLeader, stat) .. "\n"
    end

    -- Separate leaderstats (Eons / Time / Universes)
    for _, stat in ipairs(mainLeaderstats) do
        result = result .. stat .. ": " .. getValue(mainLeader, stat) .. "\n"
    end

    -- Locked stats (OtherData → Classes)
    for _, stat in ipairs(lockedStats) do
        result = result .. "Locked_" .. stat .. ": " .. getValue(classesFolder, stat) .. "\n"
    end

    result = result .. "\n"
end

if setclipboard then
    setclipboard(result)
elseif toclipboard then
    toclipboard(result)
elseif syn and syn.write_clipboard then
    syn.write_clipboard(result)
else
    warn("Clipboard not supported.")
end

print(result)
